<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">
    <title>Demande d'inscription | Sah-Tech</title>
    <link rel="stylesheet" href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css">
    <link rel="stylesheet" href="/stylesheets/gestion-compte.css">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;600&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Material+Icons|Material+Icons+Outlined|Material+Icons+Two+Tone|Material+Icons+Round|Material+Icons+Sharp" rel="stylesheet">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
    integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css" >
</head>

<body>
    <input type="checkbox" id="nav-toggle" checked="checked">
    <div class="sidebar">
      
      
        <img src="/stylesheets/img/logoapppp.png" class="sidebar_img">
        <div class="logo-details">
            <label for="nav-toggle" >
            <i class='bx bx-menu' id="btttn" ></i>
            </label>
        </div>
     
      
      
        <!--Side bar start here-->
            <ul class="nav-list">
              <li>
                 <button class="bttn"> 
                <i class="fa fa-user-md" aria-hidden="true"></i>
                  <span class="links_name">Medecins</span>
                 </button> 
                 <span class="tooltip">Dossier médical</span>
              </li>
              <li>
               <button class="bttn">
                <i class="fa fa-medkit" aria-hidden="true"></i>
                 <span class="links_name">Infirmiers</span>
               </button>
               <span class="tooltip">Examen médical</span>
             </li>
             <li>
               <button class="bttn">
                <i class="fa fa-address-book-o" aria-hidden="true"></i>
                 <span class="links_name">Assistants administratif</span>
               </button>
               <span class="tooltip">Agenda</span>
             </li>
             <li>
               <button class="bttn">
                <i class="fa fa-users" aria-hidden="true"></i>
                 <span class="links_name">Patients</span>
               </button>
               <span class="tooltip">Statistique</span>
             </li>
            </ul>
    </div>

    <div class="main-content">
        <header>
            <h2>
                <label for="nav-toggle">
                 
                </label> Les demandes
            </h2>

          
            <div class="user-wrapper">
                <img src="/stylesheets/img/medecina.png" width="40px" height="40px" alt="">
               
            </div>
        </header>

        <main>
            
            <!--Tableau des compte-->
            <div class="recent-grid">
                <div class="projects">
                    <div class="card">
                        <div class="card-header">
                            <h3 style="margin-left: 120px;">Les demandes</h3>
                            
                        </div>

                        <div class="card-body">
                            <div class="table-responsive">
                                <table width="100%">
                                    <thead>
                                        <tr>
                                            <td>Nom Prenom</td>
                                            <td>Email</td>
                                            <td>Categorie</td>
                                            <td>Accepter</td>
                                            <td>Supprimer</td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% userData.forEach(function(data){
                                        %>
                                         <tr>
                                            <td><%=data.nom%> <%=data.prenom%></td>
                                            <td><%=data.email%></td>
                                            <td><%=data.categorie%></td>
                                            <td><form action="/accepter/<%=data.email%>" method="GET"><button class="acc-btn" id="accBtn">Accepter</button></form></td>
                                            
                            
                                            <td><form action="/delete/<%=data.email%>" method="GET"> <button id="button-icon"> <span class="material-icons-outlined">
                                                delete
                                                </span> </button></form></td>
                                            </tr>
                                            <%})%>
                                    </tbody>
                                </table>
                            </div>
                        </div>

                    </div>
                </div>

                
            </div>
        </main>

    </div>
<script src="/stylesheets/fichier-js/gestion-compte.js"></script>
</body>

</html>