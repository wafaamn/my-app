<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">
    <title>Admin | Sah-Tech</title>
    <link rel="stylesheet" href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css">
    <link rel="stylesheet" href="/stylesheets/interface-admin.css">
    <link rel="stylesheet" href="https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css" >
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
    integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEHi3Sl0sibVcOQVnN" crossorigin="anonymous" />
</head>

<body>
 
    <input type="checkbox" id="nav-toggle" checked="checked">
    <div class="sidebar">
      
      
        <img src="/stylesheets/img/logoapppp.png" class="sidebar_img">
        <div class="logo-details">
            <label for="nav-toggle" >
            <i class='bx bx-menu' id="btttn" ></i>
            </label>
        </div>
     
      
      
        <!--Side bar start here-->
            <ul class="nav-list">
              <li>
                <form action="gestion-medecin" method="get">
                 <button class="bttn"> 
                 <i class="fa fa-user-md" aria-hidden="true"></i>
                  <span class="links_name">Medecins</span>
                 </button> 
                </form>
                 <span class="tooltip">Dossier médical</span>
              </li>
              <li>
                  <form method="GET" action="infermier">
               <button class="bttn">
                <i class="fa fa-medkit" aria-hidden="true"></i>
                 <span class="links_name">Infirmiers</span>
               </button></form>
               <span class="tooltip">Examen médical</span>
             </li>
             <li>
                 <form method="GET" action="assistant">
               <button class="bttn">
                <i class="fa fa-address-book-o" aria-hidden="true"></i>
                 <span class="links_name">Assistants administratif</span>
               </button></form>
               <span class="tooltip">Agenda</span>
             </li>
             <li>
                <li>
                    <form method="GET" action="gestion-patient">
                  <button class="bttn">
                   <i class="fa fa-address-book-o" aria-hidden="true"></i>
                    <span class="links_name">Patients</span>
                  </button></form>
                  <span class="tooltip">Agenda</span>
                </li>
                <li>
                 <form action="patient" method="GET">
               <button class="bttn">
                <i class="fa fa-users" aria-hidden="true"></i>
                 <span class="links_name">Demandes Inscription</span>
               </button></form>
               <span class="tooltip">Statistique</span>
             </li>
             <li>
                 <form action="creer-compte" method="GET">
                <button class="bttn">
                <i class="fa fa-plus-square" aria-hidden="true"></i>
                  <span class="links_name">Créer un compte</span>
                </button></form>
                <span class="tooltip">Statistique</span>
              </li>
            </ul>
    </div>

    <div class="main-content">
        <header>
           
            <h2>
                
                <label for="nav-toggle">
                   
                </label> Tableau de bord
            </h2>

            <div class="search-wrapper">
                <span class="las la-search"></span>
                <input type="search" placeholder="Recherche"/>
            </div>
            <div class="user-wrapper">
                <img src="/stylesheets/img/medecina.png" width="40px" height="40px" alt="">
                <div>
                    
                <button class="btn"><i class="fa fa-sign-out" aria-hidden="true"></i></button>
                      
                </div>
            </div>
        </header>

        <main>
            <div class="cards">

                <div class="card-single">
                    <%  userData6.forEach(function(result5){ %>
                    <div>
                        <h1><%=result5%></h1>
                        <span>Medecins</span>
                    </div><%})%>
                    <div>
                        <i class="fa fa-user-md" aria-hidden="true"></i>
                    </div>
                </div>

                <div class="card-single">
                    <div>
                        <h1>12</h1>
                        <span>Infirmiers</span>
                    </div>
                    <div>
                        <i class="fa fa-medkit" aria-hidden="true"></i>
                    </div>
                </div>

                <div class="card-single">
                    <div>
                        <h1>25</h1>
                        <span>Patients</span>
                    </div>
                    <div>
                        <i class="fa fa-users" aria-hidden="true"></i>
                    </div>
                </div>

                <div class="card-single">
                    <div>
                        <h1>2</h1>
                        <span>Assistant Administratif</span>
                    </div>
                    <div>
                        <i class="fa fa-address-book-o" aria-hidden="true"></i>
                    </div>
                </div>
            </div>
            <!--Tableau des compte-->
            <div class="recent-grid">
                <div class="projects">
                    <div class="card">
                        <div class="card-header">
                            <h3>Tous les comptes</h3>
                        </div>

                        <div class="card-body">
                            <div class="table-responsive">
                                <table width="100%">
                                    <thead>
                                        <tr>
                                            <td>Nom Prenom</td>
                                            <td>Email</td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <%  userData.forEach(function(result){ %>
        
                                        <tr>
                                            <td><%=result.nom%> <%=result.prenom%></td>
                                            <td><%=result.Email%></td>
                                        </tr><%})%>
                                        <%    userData2.forEach(function(result1){ %>
                                        <tr>
                                            <td><%=result1.nom%> <%=result1.prenom%></td>
                                            <td><%=result1.Email%></td>
                                        </tr><%})%>
                                        <%  userData3.forEach(function(result2){ %>
                                        <tr>
                                            <td><%=result2.nom%> <%=result2.prenom%></td>
                                            <td><%=result2.Email%></td>
                                        </tr><%})%>
                                        <% userData4.forEach(function(result3){ %>
                                        <tr>
                                            <td><%=result3.nom%> <%=result3.prenom%></td>
                                            <td><%=result3.Email%></td>
                                        </tr><%})%>
                                        <%    userData5.forEach(function(result4){
                                            %> 
                                        <tr>
                                            <td><%=result4.nom%> <%=result4.prenom%></td>
                                            <td><%=result4.Email%></td>
            
                                        </tr><%})%>
                                    </tbody>
                                </table>
                            </div>
                        </div>

                    </div>
                </div>
                
            </div>
        </main>

    </div>
    <script src="/stylesheets/fichiers-js/interface-admin.js"></script>
</body>

</html>